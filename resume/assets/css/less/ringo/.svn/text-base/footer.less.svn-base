.footer-block{
	background:@whiteMoreDarker - #040404;
	padding:20px 0;
	p{
		text-align: center;
		color:@grayLightest;
	}
	ul{
		list-style:none;
		text-align: center;
		color:@grayLightest;
		margin:0 0 20px 0;
		li{
			display:inline-block;
			margin:0 10px;
			a{
				color:@grayLightest;
				text-shadow: 0 -1px 0 rgba(255, 255, 255, 0.25);
			}
		}
	}
}